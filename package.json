{
  "name": "react-devui",
  "version": "0.0.0",
  "license": "MIT",
  "description": "React Component Library",
  "homepage": "https://github.com/xiejay97/react-devui",
  "private": true,
  "engines": {
    "node": "12.x || 14.x || 16.x",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "site:serve": "nx serve site",
    "lint:affected": "nx affected --target=lint --parallel=2",
    "lint:affected:fix": "yarn lint:affected --fix",
    "lint:style": "stylelint **/*.{css,scss,html,md}",
    "lint:style:fix": "yarn lint:style --fix",
    "test:affected": "nx affected --target=test --parallel=2",
    "build:affected": "nx affected --target=build --parallel=2",
    "build:ui": "nx build ui",
    "build:site": "nx build site",
    "build:executors": "tsc --project tools/tsconfig.json",
    "build:helper": "node ./tools/ui-helper.js",
    "build:icons": "node ./tools/ui-icons.js",
    "release": "standard-version",
    "prepare": "husky install && chmod ug+x .husky/*",
    "prepublishOnly": "nx build ui",
    "check-update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "core-js": "^3.21.1",
    "highlight.js": "^11.5.0",
    "i18next": "^21.6.14",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "react": "18.0.0-rc.2",
    "react-dom": "18.0.0-rc.2",
    "react-i18next": "^11.16.1",
    "react-router-dom": "^6.2.2",
    "regenerator-runtime": "^0.13.9",
    "rfs": "^9.0.6",
    "rxjs": "^7.5.5",
    "scrollview-resize": "^1.0.2",
    "stylelint-config-recess-order": "^3.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nrwl/cli": "13.8.8",
    "@nrwl/eslint-plugin-nx": "13.8.8",
    "@nrwl/jest": "13.8.8",
    "@nrwl/linter": "13.8.8",
    "@nrwl/nx-cloud": "13.1.6",
    "@nrwl/react": "13.8.8",
    "@nrwl/tao": "13.8.8",
    "@nrwl/web": "13.8.8",
    "@nrwl/workspace": "13.8.8",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "13.0.0-alpha.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.5.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.6",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss-html": "^1.3.0",
    "postcss-markdown": "^1.2.0",
    "prettier": "^2.6.1",
    "react-test-renderer": "^17.0.2",
    "rxjs-for-await": "^1.0.0",
    "sass": "^1.49.9",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.3",
    "yaml-front-matter": "^4.1.1"
  }
}
